    <Match>
        <Bug pattern="OI_OPTIONAL_ISSUES_USES_IMMEDIATE_EXECUTION" />
        <Class name="~.*\.shape\.Align.*" />
    </Match>
    <!-- False positive instances in ViewText -->
    <Match>
        <Bug pattern="HES_EXECUTOR_NEVER_SHUTDOWN" />
